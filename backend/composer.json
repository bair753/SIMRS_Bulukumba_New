{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "fzaninotto/faker": "^1.6",
        "barryvdh/laravel-cors": "^0.8.0",
        "namshi/jose": "7.0.*",
        "jenssegers/date": "^3.2",
        "webpatser/laravel-uuid": "2.*",
        "riskihajar/terbilang": "1.0.5",
        "lcobucci/jwt": "3.3",
        "jimmyjs/laravel-report-generator": "^1.0",
        "dompdf/dompdf": "^0.8.3",
        "dmeys/laravel-whatsapp": "dev-master",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "illuminate/support": "5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x|5.7.x",
        "bacon/bacon-qr-code": "^2.0",
        "endroid/installer": "^1.0.3",
        "khanamiryan/qrcode-detector-decoder": "^1.0.2",
        "myclabs/php-enum":"^1.5",
        "symfony/options-resolver": "^2.7|^3.0|^4.0",
        "symfony/property-access": "^2.7|^3.0|^4.0",
        "endroid/qr-code": "^3.5",
        "codedge/laravel-fpdf": "^1.2",
        "guzzlehttp/guzzle": "^6.5",
        "twilio/sdk": "^6.11",
        "neoxia/laravel-sftp": "^1.0",
        "mpdf/mpdf": ">=7.0",
        "nullpunkt/lz-string-php": "^1.3"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "laracasts/generators": "^1.1",
        "laravel/homestead": "^3.0",
        "doctrine/dbal": "^2.5"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
